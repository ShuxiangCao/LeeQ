{
  "setup_type": "hardware",
  "setup_class": "QubicLBNLSetup",
  "config": {
    "hardware_config": "/etc/leeq/hardware/lab1_config.json",
    "calibration_database": "/var/leeq/calibrations/lab1.db",
    "qubits": ["q0", "q1", "q2", "q3"],
    "coupling_map": [[0, 1], [1, 2], [2, 3], [0, 3]],
    "control_electronics": {
      "awg_ip": "192.168.1.100",
      "digitizer_ip": "192.168.1.101",
      "lo_ip": "192.168.1.102"
    },
    "safety_limits": {
      "max_amplitude": 1.0,
      "max_frequency_offset": 100e6,
      "max_pulse_duration": 10e-6
    }
  },
  "grpc": {
    "port": 50051,
    "max_workers": 4,
    "max_message_length": 100 * 1024 * 1024,
    "compression": "gzip"
  },
  "logging": {
    "level": "WARNING", 
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "/var/log/leeq/epii.log",
    "max_bytes": 10485760,
    "backup_count": 5
  },
  "experiment_timeout": 600,
  "parameter_validation": true,
  "safety_checks": {
    "enabled": true,
    "max_experiment_duration": 3600,
    "hardware_interlocks": true
  },
  "monitoring": {
    "enabled": true,
    "metrics_port": 8080,
    "health_check_interval": 30
  }
}